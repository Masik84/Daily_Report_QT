/* Улучшенная видимость чекбоксов */
QCheckBox::indicator {
    width: 16px;
    height: 16px;
}

QCheckBox::indicator:unchecked {
    border: 2px solid #f09d54;
    background-color: #f8f8f2;
    border-radius: 3px;
}

QCheckBox::indicator:checked {
    border: 2px solid #f28223;
    background-color: #f28223;
    border-radius: 3px;
    image: url(:/icon/icon/check.svg);
}

QCheckBox::indicator:hover {
    border: 2px solid #f28223;
}

QComboBox {
	background-color: #f8f8f2;
	border-radius: 5px;
	border: 2px solid #f09d54;
	padding-left: 10px;
	selection-color: #f28223;
	selection-background-color: #f28223;
    color: #262626;
	font: 9pt &quot;Tahoma&quot;;
}
QComboBox:hover {
	border: 2px solid #f28223;
	
}
